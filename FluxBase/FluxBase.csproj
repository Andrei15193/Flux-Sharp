<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp1.0;netcoreapp1.1;netcoreapp2.0;netcoreapp2.1;netcoreapp2.2;net20;net35;net40;net45;net451;net452;net46;net461;net462;net47;net471;net472;netstandard1.0;netstandard1.1;netstandard1.2;netstandard1.3;netstandard1.4;netstandard1.5;netstandard1.6;netstandard2.0</TargetFrameworks>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
    <TreatSpecificWarningsAsErrors />
    <Description>Provides core types and functionality to implement applications using the Flux architecture with .NET.</Description>
    <Authors>Andrei Fangli</Authors>
    <Copyright>Andrei Fangli</Copyright>
    <PackageProjectUrl>https://github.com/Andrei15193/FluxBase</PackageProjectUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Andrei15193/FluxBase.git</RepositoryUrl>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageTags>flux architecture user interface ui</PackageTags>
    <PackageIconUrl>https://raw.githubusercontent.com/Andrei15193/FluxBase/master/package_logo.png</PackageIconUrl>
    <PackageReleaseNotes>Added middleware (similar to Redux middleware) to dispatcher covering only synchronous dispatch flows.
Added async dispatcher and middleware covering only asynchronous dispatch flows.
Removed ActionData as base class for dispatched action, any object can be used as an action now.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE" Pack="true" PackagePath="" Visible="false" />
  </ItemGroup>

</Project>